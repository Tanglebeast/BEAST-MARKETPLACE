{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "beast-marketplace-iwhbev.vercel.app"
          }
        ],
        "destination": "/"
      }
    ]
  }
  